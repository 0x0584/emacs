(deftheme c-anti-zenburn
  "Created 2016-09-14.")

(custom-theme-set-faces
 'c-anti-zenburn
 '(default ((t (:family "DejaVu Sans Mono" :foundry "PfEd" :width normal :height 113 :weight normal :slant normal :underline nil :overline nil :strike-through nil :box nil :inverse-video nil :foreground "#232333" :background "#c0c0c0" :stipple nil :inherit nil))))
 '(cursor ((t (:foreground "#232333" :background "#000010"))))
 '(fixed-pitch ((t (:family "Monospace"))))
 '(variable-pitch ((((type w32)) (:font "-outline-Arial-normal-normal-normal-sans-*-*-*-*-p-*-iso8859-1")) (t (:family "Sans Serif"))))
 '(escape-glyph ((t (:weight bold :foreground "#0f2050"))))
 '(minibuffer-prompt ((t (:foreground "#0f2050"))))
 '(highlight ((t (:background "#c7c7c7"))))
 '(region ((t (:background "#d4d4d4"))))
 '(shadow ((((class color grayscale) (min-colors 88) (background light)) (:foreground "grey50")) (((class color grayscale) (min-colors 88) (background dark)) (:foreground "grey70")) (((class color) (min-colors 8) (background light)) (:foreground "green")) (((class color) (min-colors 8) (background dark)) (:foreground "yellow"))))
 '(secondary-selection ((t (:background "#a0a0a0"))))
 '(trailing-whitespace ((t (:background "#336c6c"))))
 '(font-lock-builtin-face ((t (:weight bold :foreground "#232333"))))
 '(font-lock-comment-delimiter-face ((t (:foreground "#a080a0"))))
 '(font-lock-comment-face ((t (:foreground "#806080"))))
 '(font-lock-constant-face ((t (:foreground "#401440"))))
 '(font-lock-doc-face ((t (:foreground "#603a60"))))
 '(font-lock-function-name-face ((t (:foreground "#6c1f1c"))))
 '(font-lock-keyword-face ((t (:weight bold :foreground "#0f2050"))))
 '(font-lock-negation-char-face ((t (:weight bold :foreground "#0f2050"))))
 '(font-lock-preprocessor-face ((t (:foreground "#6b400c" :underline t))))
 '(font-lock-regexp-grouping-backslash ((t (:weight bold :foreground "#806080"))))
 '(font-lock-regexp-grouping-construct ((t (:weight bold :foreground "#0f2050"))))
 '(font-lock-string-face ((t (:foreground "#336c6c"))))
 '(font-lock-type-face ((t (:inherit default :foreground "#834744" :slant normal))))
 '(font-lock-variable-name-face ((t (:foreground "#205070"))))
 '(font-lock-warning-face ((t (:weight bold :foreground "#2f4070"))))
 '(button ((t (:underline (:color foreground-color :style line)))))
 '(link ((t (:weight bold :underline (:color foreground-color :style line) :foreground "#0f2050"))))
 '(link-visited ((t (:weight normal :underline (:color foreground-color :style line) :foreground "#2f4070"))))
 '(fringe ((t (:foreground "#232333" :background "#b0b0b0"))))
 '(header-line ((t (:box (:line-width -1 :color nil :style released-button) :foreground "#0f2050" :background "#d4d4d4"))))
 '(tooltip ((t (:foreground "#232333" :background "#b0b0b0"))))
 '(mode-line ((t (:box (:line-width -1 :color nil :style released-button) :foreground "#704d70" :background "#d4d4d4"))))
 '(mode-line-buffer-id ((t (:weight bold :foreground "#0f2050"))))
 '(mode-line-emphasis ((t (:weight bold))))
 '(mode-line-highlight ((((class color) (min-colors 88)) (:box (:line-width 2 :color "grey40" :style released-button))) (t (:inherit (highlight)))))
 '(mode-line-inactive ((t (:box (:line-width -1 :color nil :style released-button) :foreground "#a080a0" :background "#c7c7c7"))))
 '(isearch ((t (:weight bold :foreground "#2f4070" :background "#a0a0a0"))))
 '(isearch-fail ((t (:foreground "#232333" :background "#73acac"))))
 '(lazy-highlight ((t (:weight bold :foreground "#2f4070" :background "#c7c7c7"))))
 '(match ((t (:weight bold :foreground "#205070" :background "#d4d4d4"))))
 '(next-error ((t (:inherit (region)))))
 '(query-replace ((t (:inherit (isearch))))))

(provide-theme 'c-anti-zenburn)
